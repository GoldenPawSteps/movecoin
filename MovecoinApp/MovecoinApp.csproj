  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\src\Models\Models.csproj" />
    <ProjectReference Include="..\src\ViewModels\ViewModels.csproj" />
    <ProjectReference Include="..\src\Services\Services.csproj" />
  </ItemGroup>

</Project>