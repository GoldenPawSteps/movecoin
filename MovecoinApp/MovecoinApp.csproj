<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0-ios;net6.0-android;net6.0-maccatalyst</TargetFramework>
    <RootNamespace>MovecoinApp</RootNamespace>
    <AssemblyName>MovecoinApp</AssemblyName>
    <ApplicationTitle>Movecoin Challenge App</ApplicationTitle>
    <ApplicationId>com.example.movecoinapp</ApplicationId>
